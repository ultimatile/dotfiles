[alias]
  al = config --get-regexp alias
  aliases = config --get-regexp alias
  pl = pull
  ps = push
  st = status
  sts = status -s
  clr = clone --recursive
  cl = clone
  sw = switch
  c = commit
  cam = commit -am
  amm  = commit --amend -m
[help]
	# Automatically correct and execute mistyped commands.
	autocorrect = 1
[core]
	editor = nvim
